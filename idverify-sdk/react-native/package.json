    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "react": "18.2.0",
    "react-native": "^0.76.5",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
